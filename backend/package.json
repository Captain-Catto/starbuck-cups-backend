{
  "name": "starbucks-backend",
  "version": "1.0.0",
  "description": "Starbucks Cup E-commerce Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run db:generate && tsc",
    "start": "node dist/index.js",
    "postbuild": "npm run db:deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "seed:sample": "ts-node scripts/seed-sample-data.ts",
    "seed:products": "ts-node scripts/seed-200-products.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "starbucks",
    "ecommerce",
    "api",
    "typescript",
    "express"
  ],
  "author": "Starbucks Shop Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.16.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/redis": "^4.0.10",
    "@types/socket.io": "^3.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "prisma": "^6.16.0",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "uuid": "^10.0.0",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.4",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1"
  }
}
